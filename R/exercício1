# Crie os objetos:
# nome, com seu nome (não esqueça das aspas);
# idade, com a sua idade;
# ano, com seu ano de nascimento;
# r sendo TRUE se você já conhecia o R, e FALSE caso contrário

#criando objetos

nome = 'Giovana'
idade = 22
ano = 2000
r = FALSE

# Execute as seguintes operações e comente os resultados:
idade + ano # resultado = 2022
ano + r # resultado = 2000
idade - r # resultado = 22
r*nome # resultado = "Error in r * nome : non-numeric argument to binary operator"
rep(nome, idade) # resultado = "Giovana" 22 vezes
